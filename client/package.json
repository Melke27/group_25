    "mapbox-gl": "^3.14.0",
    "mongoose": "^7.8.7",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^15.6.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
